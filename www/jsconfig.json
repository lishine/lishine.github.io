{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ES6",
        "moduleResolution": "node",
        "jsx": "react",
        "skipLibCheck": true,
        "allowUnusedLabels": true,
    },
    "exclude": [
        "**/node_modules/**",
        "**/.next/**",
        "**/dist/**",
        "**/db/**",
        "**/.history/**",
        "**/.git/**",
        "**/not_used/**",
    ]
}